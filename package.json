{
  "name": "@tweetback/tweetback",
  "version": "1.0.0",
  "private": true,
  "description": "Take ownership of your Twitter data and host your own Twitter archive.",
  "scripts": {
    "clean": "rm -rf .cache",
    "import": "node database/import-from-archive.js",
    "import-without-circles": "npm run import -- removecircletweets",
    "fetch-new-data": "node database/fetchFromApi.js",
    "build": "npx @11ty/eleventy --quiet",
    "start": "npx @11ty/eleventy --quiet --serve"
  },
  "keywords": [],
  "contributors": [
    "Zach Leatherman <zachleatherman@gmail.com> (https://zachleat.com/)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.1",
    "@tweetback/canonical": "^2.0.37",
    "country-emoji": "^1.5.6",
    "dotenv": "^16.3.1",
    "emoji-regex": "^10.2.1",
    "entities": "^4.5.0",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "pagefind": "^0.10.6",
    "parse-domain": "^5.0.0",
    "sentiment": "^5.0.2",
    "sqlite3": "^5.1.6",
    "swearjar": "^0.2.0",
    "twitter-lite": "^1.1.0"
  },
  "dependencies": {
    "@11ty/is-land": "^4.0.0"
